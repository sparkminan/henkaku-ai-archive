# 🚀 GAS API Configuration (推奨方法)
# Google Apps Script Web App URL
NEXT_PUBLIC_GAS_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# 🔄 Legacy: Direct Airtable API (フォールバック用)
AIRTABLE_API_KEY=your_api_key_here
AIRTABLE_BASE_ID=your_base_id_here
AIRTABLE_SESSIONS_TABLE_NAME=Sessions

# Enable real-time Airtable integration
NEXT_PUBLIC_USE_AIRTABLE=true